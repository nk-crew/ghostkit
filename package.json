{
  "name": "ghostkit",
  "title": "Ghost Kit",
  "version": "2.22.0",
  "description": "Blocks collection and extensions for Gutenberg",
  "license": "GPL-2.0",
  "author": "nK <https://nkdev.info>",
  "scripts": {
    "dev": "wpeg --build --watch",
    "build": "wpeg --build",
    "production": "npm run make-pot && npm run make-json && wpeg --build --zip",
    "php-lint": "composer phpcs",
    "php-lint-fix": "composer phpcbf",
    "css-lint": "stylelint --custom-syntax postcss-scss \"src/**/*.scss\"",
    "css-lint-fix": "stylelint --fix --custom-syntax postcss-scss \"src/**/*.scss\"",
    "js-lint": "eslint --ext .js src/",
    "js-lint-fix": "eslint --ext .js --fix src/",
    "make-pot": "composer wp -- i18n make-pot ./src ./src/languages/ghostkit.pot --skip-audit --exclude=vendor,vendors,node_modules --headers='{\"Last-Translator\":\"nK\",\"Report-Msgid-Bugs-To\":\"https://github.com/nk-o/ghostkit/issues\"}'",
    "make-json": "po2json ./src/languages/ghostkit.pot ./src/languages/ghostkit.json --format=jed1.x --pretty --domain=@@text_domain",
    "prepare": "husky install",
    "postinstall": "composer install"
  },
  "devDependencies": {
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "@svgr/webpack": "^5.5.0",
    "base64-inline-loader": "^2.0.1",
    "eslint-config-nk": "^1.0.8",
    "husky": "^7.0.4",
    "json-file": "^0.1.0",
    "lint-staged": "^12.1.2",
    "md5": "^2.3.0",
    "micromatch": "^4.0.4",
    "path": "^0.12.7",
    "po2json": "^0.4.5",
    "prettier-config-nk": "^1.0.2",
    "stylelint-config-nk": "^1.0.8",
    "wpeg": "^1.2.6"
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^4.1.2",
    "@wordpress/base-styles": "^2.1.0",
    "ace-builds": "^1.4.13",
    "classnames": "^2.3.1",
    "deep-equal": "^2.0.5",
    "gist-simple": "^1.0.1",
    "gmaps": "^0.4.25",
    "gutenberg-react-select-styles": "0.0.2",
    "jarallax": "^1.12.8",
    "markdown-it": "^12.2.0",
    "parsleyjs": "^2.9.2",
    "qs": "^6.10.1",
    "raf-schd": "^4.0.3",
    "react-ace": "^9.5.0",
    "react-google-maps": "^9.4.5",
    "react-lazyload": "^3.2.0",
    "react-masonry-component": "^6.3.0",
    "react-select": "^5.2.1",
    "react-select-virtualized": "^4.2.0",
    "react-virtualized": "^9.22.3",
    "recompose": "^0.30.0",
    "scriptjs": "^2.5.9",
    "scrollreveal": "^4.0.9",
    "shorthash": "0.0.2",
    "slugify": "^1.6.2",
    "striptags": "^3.2.0",
    "swiper": "^6.5.9",
    "throttle-debounce": "^3.0.1"
  }
}
