{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "ghostkit/markdown",
  "category": "ghostkit",
  "title": "Markdown",
  "description": "Lightweight markup language with plain-text-formatting syntax.",
  "keywords": ["formatting", "markup", "md"],
  "supports": {
    "ghostkit": {
      "animation": {
        "reveal": true,
        "scroll": true,
        "loop": true,
        "mouse": true
      }
    },
    "html": false,
    "className": true,
    "anchor": false
  },
  "attributes": {
    "content": {
      "type": "string"
    }
  }
}
