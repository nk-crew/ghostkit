{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "ghostkit/tabs-tab-v2",
  "parent": ["ghostkit/tabs-v2"],
  "category": "ghostkit",
  "title": "Tab",
  "description": "A single tab within a tabs block.",
  "supports": {
    "ghostkit": {
      "animation": {
        "reveal": true,
        "scroll": true,
        "loop": true,
        "mouse": true
      }
    },
    "html": false,
    "className": false,
    "anchor": true,
    "reusable": false,
    "spacing": {
      "blockGap": true,
      "__experimentalDefaultControls": {
        "blockGap": true
      }
    },
    "__experimentalLayout": true
  },
  "attributes": {
    "slug": {
      "type": "string"
    }
  }
}
