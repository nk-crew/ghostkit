{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "ghostkit/form-submit-button",
  "parent": ["ghostkit/form"],
  "category": "ghostkit",
  "title": "Submit Button",
  "description": "Form submit button.",
  "supports": {
    "ghostkit": {
      "animation": {
        "reveal": true,
        "scroll": true,
        "loop": true,
        "mouse": true
      }
    },
    "html": false,
    "className": false,
    "anchor": true,
    "reusable": false
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "none"
    }
  }
}
