{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ghostkit/form",
  "category": "ghostkit",
  "title": "Form",
  "supports": {
    "ghostkit": {
      "animation": {
        "reveal": true,
        "scroll": true,
        "loop": true,
        "mouse": true,
        "hover": true
      }
    },
    "html": false,
    "className": false,
    "anchor": false,
    "align": ["wide", "full"],
    "spacing": {
      "blockGap": true,
      "__experimentalDefaultControls": {
        "blockGap": true
      }
    },
    "__experimentalLayout": true,
    "layout": true
  }
}
