{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "ghostkit/changelog",
  "category": "ghostkit",
  "title": "Changelog",
  "description": "Show the changes log of your product.",
  "keywords": ["changelog", "log"],
  "supports": {
    "ghostkit": {
      "animation": {
        "reveal": true,
        "scroll": true,
        "loop": true,
        "mouse": true
      }
    },
    "html": false,
    "className": false,
    "anchor": true,
    "align": ["wide", "full"],
    "spacing": {
      "blockGap": true,
      "__experimentalDefaultControls": {
        "blockGap": true
      }
    },
    "__experimentalLayout": true
  },
  "attributes": {
    "version": {
      "type": "string",
      "default": ""
    },
    "date": {
      "type": "string",
      "default": ""
    }
  }
}
